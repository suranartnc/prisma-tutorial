type Link {
  id: ID! @id
  description: String!
  url: String!
  votes: [Vote!]!
  createdAt: DateTime! @createdAt
  postedBy: User
}

type User {
  id: ID! @id
  name: String!
  email: String! @unique
  password: String!
  links: [Link!]!
  votes: [Vote!]!
}

type Vote {
  id: ID! @id
  link: Link!
  user: User!
}